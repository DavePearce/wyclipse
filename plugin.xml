<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="wyclipse.editor.Editor"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            extensions="whiley"
            icon="icons/sample.gif"
            id="wyclipse.editor.Editor"
            name="Whiley File Editor">
      </editor>
   </extension>
   <extension
         id="whileyBuilder"
         name="Whiley Project Builder"
         point="org.eclipse.core.resources.builders">
      <builder
            hasNature="true">
         <run
               class="wyclipse.builder.Builder">
         </run></builder>
   </extension>
   <extension
         id="WhileyNature"
         name="Whiley Nature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="wyclipse.ProjectNature1">
         </run>
      </runtime>
      <builder
            id="wyclipse.whileyBuilder">
      </builder>
   </extension>
   <extension
         id="WhileyNewWizard"
         name="Whiley New Module Wizard"
         point="org.eclipse.ui.newWizards">
      <category
            id="wyclipse.whileyCategory"
            name="Whiley">
      </category>
      <wizard
            category="wyclipse.whileyCategory"
            class="wyclipse.wizards.NewModuleWizard"
            icon="icons/newmodule_wiz.gif"
            id="wyclipse.newModuleWizard"
            name="Whiley Module"
            project="false">
      </wizard>
      <primaryWizard
            id="wyclipse.newModuleWizard">
      </primaryWizard>
      <wizard
            category="wyclipse.whileyCategory"
            class="wyclipse.wizards.NewProjectWizard"
            icon="icons/newwprj_wiz.gif"
            id="wyclipse.newProjectWizard"
            name="Whiley Project"
            project="true">
      </wizard>
      <primaryWizard
            id="wyclipse.newProjectWizard">
      </primaryWizard>
   </extension>


</plugin>
