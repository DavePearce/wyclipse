<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="wyclipse.editor.Editor"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            extensions="whiley"
            icon="icons/sample.gif"
            id="wyclipse.editor.Editor"
            name="Whiley File Editor">
      </editor>
   </extension>
   <extension
         id="whileyBuilder"
         name="Whiley Project Builder"
         point="org.eclipse.core.resources.builders">
      <builder
            hasNature="true">
         <run
               class="wyclipse.builder.Builder">
         </run></builder>
   </extension>
   <extension
         id="WhileyNature"
         name="Whiley Nature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="wyclipse.ProjectNature1">
         </run>
      </runtime>
      <builder
            id="wyclipse.whileyBuilder">
      </builder>
   </extension>


</plugin>
